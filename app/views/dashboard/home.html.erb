<h1>Dashboard#home</h1>
<p>Find me in app/views/dashboard/home.html.erb</p>
<% current_user.events.each.with_index(1) do |event, index| %>
  <h2><span>[<%= index %>]</span> <%= event.name %></h2>
  <%= link_to "Show Event", event %> | 
  <%= button_to "Leave Event", joinable_event_path(event) %>
<% end %>
<%= link_to "Logout", destroy_user_session_path, method: :delete, data: { turbo_method: :delete } %>
